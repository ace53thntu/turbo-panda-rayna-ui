{
  "name": "@edm/preset",
  "version": "0.0.1",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts --format=esm,cjs --dts --external @pandacss/dev",
    "build-fast": "tsup src/index.ts --format=esm,cjs --no-dts",
    "dev": "pnpm build --watch",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@edm/typescript-config": "workspace:*",
    "@pandacss/dev": "^0.30.1",
    "@pandacss/preset-panda": "^0.30.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}